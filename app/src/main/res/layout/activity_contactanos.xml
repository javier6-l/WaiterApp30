<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@color/gris2"
    android:layout_height="match_parent"
    tools:context=".Contactanos">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textAlignment="center"
        android:background="@color/gris"
        android:orientation="horizontal">
        <Button
            android:layout_width="70sp"
            android:layout_marginTop="2sp"
            android:background="@drawable/boton_redondo"
            android:layout_height="40dp"
            android:textColor="#ffffff"
            android:onClick="atras"
            android:text="Atras">
        </Button>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Informacion y Contactos"
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:layout_marginTop="5sp"
            android:layout_marginLeft="50dp"
            android:textSize="20sp"></TextView>
    </LinearLayout>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <TextView
        android:id="@+id/textC2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cocina:"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:textColor="#ffffff"
        android:textSize="25dp"></TextView>
    <TextView
        android:id="@+id/china"
        android:textColor="#ffffff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textC2"
        android:layout_marginLeft="45sp"
        android:layout_marginTop="10dp"
        android:text="China"
        android:textSize="19sp"></TextView>
    <TextView
        android:id="@+id/Continental"
        android:textColor="#ffffff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/china"
        android:layout_marginLeft="45sp"
        android:layout_marginTop="10dp"
        android:text="Continental"
        android:textSize="19sp"></TextView>
    <TextView
        android:id="@+id/irlandesa"
        android:textColor="#ffffff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Continental"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="45sp"
        android:text="Irlandesa"
        android:textSize="19sp"></TextView>
    <TextView
        android:layout_width="wrap_content"
        android:textColor="#ffffff"
        android:layout_height="wrap_content"
        android:layout_below="@+id/irlandesa"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="45sp"
        android:text="Mexicana"
        android:textSize="19sp"></TextView>
</LinearLayout>
    <LinearLayout
        android:layout_marginTop="15dp"
